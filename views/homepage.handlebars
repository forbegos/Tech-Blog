<!-- Loop over each object in the entries array -->
{{#each entries as |entry|}}
  <div class="row mb-4 project">
    <div class="col-md-5">
      <div class="card" style="width: 60rem;">
        <div class="card-header">
          <h3>
            {{entry.title}}
          </h3>
        </div>
        <div class="card-body">
          <p class="card-text"> {{entry.content}}</p>
        </div>
      </div>
    </div>
  </div>
{{/each}}
<div>
  <a href="/entry" class="btn btn-primary stretched-link">New Post</a>
</div>